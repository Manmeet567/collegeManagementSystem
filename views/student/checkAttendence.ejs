<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student | Check Attendence</title>
    <link rel="stylesheet" href="/StudentDashboard.css">
    <link rel="stylesheet" href="/StudentAttendence.css">
    <link rel="stylesheet" href="/adminNavbar.css">
</head>
<body>
    <div class="dashboard">
        <%- include('../partials/studentDashboard.ejs') %>
    </div>
    <main>
        <%- include('../partials/studentNavbar.ejs') %>
        <div class="attendence">
            <div class="card">
                <h1>Overall Attendence</h1>
                <div class="circle">
                    <p><%= student.attendence %>%</p>
                </div>
                <div class="bar"><div class="inner-bar"></div></div>
            </div>
        </div>
    </main>

    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
</body>
</html>